{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:worker": "pnpm run build && opennextjs-cloudflare preview",
    "dev:worker:remote": "pnpm run build && opennextjs-cloudflare preview --remote",
    "build": "next build && opennextjs-cloudflare build --skipNextBuild",
    "start": "pnpm run dev:worker",
    "deploy": "pnpm run build && opennextjs-cloudflare deploy",
    "test": "vitest run",
    "lint": "eslint",
    "lint:code": "eslint",
    "lint:type": "tsc --noEmit",
    "clean": "rm -rf dist .turbo .next .open-next",
    "clean:all": "pnpm clean && rm -rf node_modules"
  },
  "dependencies": {
    "@doujin/core": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "postcss": "^8.5.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9.39.0",
    "eslint-config-next": "16.1.6",
    "happy-dom": "^18.0.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.63.0"
  }
}
