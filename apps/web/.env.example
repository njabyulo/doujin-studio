# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/doujin

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Google AI (Gemini)
# Prefer GOOGLE_GENERATIVE_AI_API_KEY (used by image + video generation + proxy).
GOOGLE_GENERATIVE_AI_API_KEY=your-gemini-api-key

# Optional fallbacks (supported by some helpers)
GOOGLE_AI_API_KEY=
GEMINI_API_KEY=
GOOGLE_API_KEY=

# AWS (used for signed S3 uploads + building public S3 URLs)
AWS_REGION=us-east-1

# Local dev credentials (optional if you use AWS_PROFILE or an IAM role)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=
