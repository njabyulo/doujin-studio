############################################
# Local dev vars (Cloudflare Workers)
# - Non-sensitive values can stay in wrangler.toml [vars]
# - Sensitive values must be injected via .dev.vars locally and
#   via `wrangler secret put` in Cloudflare
############################################

# Secrets
AUTH_SECRET=
GEMINI_API_KEY=

# Non-secret configuration
AI_INTERPRET_MODEL=gemini-3-flash-preview

# Optional local overrides for wrangler.toml [vars]
APP_ENV=development
CORS_ORIGIN=http://localhost:3000
GIT_SHA=dev-local
